{
  "name": "discord-activity-server",
  "version": "1.0.0",
  "description": "Simple Discord Activity server with WebSockets",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm\" node --experimental-specifier-resolution=node server.ts",
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "keywords": [
    "discord",
    "activity",
    "websocket"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "ws": "^8.16.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2"
  }
}
